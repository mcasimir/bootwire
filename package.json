{
  "name": "bootwire",
  "version": "1.0.0",
  "description": "Application and dependencies bootstrap for node.js",
  "main": "lib/index.js",
  "scripts": {
    "docs": "npm-run-series docs:jsdoc docs:toc",
    "docs:toc": "markdown-toc -i README.md",
    "docs:jsdoc": "jsdoc2md -d 3 -c .jsdocrc lib/**/*.js | node-injectmd -t docs -i README.md",
    "depcheck": "npm-run-series depcheck:ci depcheck:nsp",
    "depcheck:ci": "depcheck-ci",
    "depcheck:nsp": "nsp check --output summary",
    "lint": "eslint . --quiet",
    "test": "NODE_ENV=test nyc mocha --timeout 10000 'test/**/*.spec.js'",
    "test:ci": "npm-run-series lint depcheck test"
  },
  "precommit": "test:ci",
  "license": "UNLICENSED",
  "dependencies": {},
  "devDependencies": {
    "depcheck-ci": "1.0.1",
    "eslint": "4.1.1",
    "eslint-config-google": "0.8.0",
    "eslint-plugin-mocha": "4.11.0",
    "eslint-plugin-no-async-without-await": "1.2.0",
    "injectmd": "1.0.0",
    "jsdoc-strip-async-await": "0.1.0",
    "jsdoc-to-markdown": "3.0.0",
    "markdown-toc": "1.1.0",
    "mocha": "3.4.2",
    "npm-run-series": "1.0.0",
    "nsp": "2.6.3",
    "nyc": "11.0.3",
    "pre-commit": "1.2.2",
    "release-flow": "1.2.1",
    "sinon": "2.3.6"
  }
}
